@ARTICLE{Hawkey2020,
author={Hawkey, A.B. and Glazer, L. and Dean, C. and Wells, C.N. and Odamah, K.-A. and Slotkin, T.A. and Seidler, F.J. and Levin, E.D.},
title={Adult exposure to insecticides causes persistent behavioral and neurochemical alterations in zebrafish},
