@article{ WOS:000332053800002,
Author = {Pandey, Manish Raj and Guo, Huarong},
Title = {Evaluation of cytotoxicity, genotoxicity and embryotoxicity of
   insecticide propoxur using flounder gill (FG) cells and zebrafish
   embryos},
Journal = {TOXICOLOGY IN VITRO},
Year = {2014},
Volume = {28},
Number = {3},
Pages = {340-353},
Month = {APR},
Abstract = {Cytotoxicity, genotoxicity and embryotoxicity of carbamate insecticide
   propoxur were evaluated using flounder gill (FG) cells and zebrafish
   embryos. The cytotoxicity of propoxur in FG cells was analyzed by MU,
   neutral red uptake (NRU), lactate dehydrogenase (LDH) release and
   Hoechst 33342 and propidiurn iodide double staining, and acute cytotoxic
   effects were observed in a concentration-dependent manner. The 24 h-IC50
   values of 89.96 +/- 1.04, 103.4 +/- 1.14 and 86.59 +/- 1.13 mu g/ml
   propoxur were obtained by NRU and LDH assays, respectively. The lethal
   effects were induced in FG cells mainly through necrosis but not
   apoptosis as evidenced by double fluorescence staining. Comet assay
   showed weak genotoxic effects and statistically significant DNA damages
   were recorded in the cells exposed to highest tested concentration of 75
   mu g/ml propoxur (p <0.05). Propoxur exerted obvious acute toxic effects
   on the survival, spontaneous movement, hatching and heart rate, and
   development (yolk and pericardial sac edema) of zebrafish embryos in
   both time- and concentration-dependent manner only at >= 100 mu g/ml.
   The corresponding 24 h-, 48 h- and 96 h-LC50 values of propoxur in
   zebrafish embryos were 166.4 +/- 1.06, 146.3 +/- 1.07 and 134.8 +/- 1.06
   mu g/ml, respectively. The above data obtained suggest a low acute
   toxicity of propoxur to the in vitro cultured FG cells and zebrafish
   embryos. (C) 2013 Elsevier Ltd. All rights reserved.},
DOI = {10.1016/j.tiv.2013.11.010},
ISSN = {0887-2333},
Unique-ID = {WOS:000332053800002},
}

@article{ WOS:000804651900001,
Author = {Victoria, Shayla and Hein, Megan and Harrahy, Elisabeth and King-Heiden,
   Tisha C.},
Title = {Potency matters: Impacts of embryonic exposure to nAChR agonists
   thiamethoxam and nicotine on hatching success, growth, and neurobehavior
   in larval zebrafish},
Journal = {JOURNAL OF TOXICOLOGY AND ENVIRONMENTAL HEALTH-PART A-CURRENT ISSUES},
Year = {2022},
Volume = {85},
Number = {18},
Pages = {767-782},
Month = {SEP 17},
Abstract = {Thiamethoxam (TM) is a neonicotinoid insecticide that acts as a
   nicotinic acetylcholine receptor (nAChR) agonist. While designed to
   specifically target invertebrate nAChRs, recent studies have reported
   adverse effects of neonicotinoid exposure in early life-stage fish. This
   study examined the health and neurobehavioral impacts of chronic
   exposure to various concentrations of TM or nicotine (NIC) in early life
   zebrafish (Danio rerio) in conjunction with in-silico molecular docking
   to compare their ligand-receptor interactions with vertebrate nAChR.
   Chronic exposure to both reduced survival by approximately 20\% (163 mu
   g TM/l) and 25-100\% (>= 0.49 mu g NIC/l). Hatching and growth were
   impaired following exposure to >= 0.21 mu g TM/l or 4.9 mu g NIC/l. Both
   TM and NIC produced morphological and behavioral indicators of
   neurotoxicity, with more potent effects following NIC exposure. NIC
   impaired embryonic motor activity by 40\% (49 mu g NIC/l), while both TM
   and NIC significantly altered predator escape response in larvae,
   specifically the latency and the initial burst movement of the response
   were impacted. Molecular docking predicted variations in the type and
   strength of interactions that occur between NIC or TM and vertebrate
   nAChR. These findings demonstrate that chronic exposure to TM might
   impact general health and neurobehavior of early-stage zebrafish. Our
   data support hypotheses that TM presents low affinity for vertebrate
   nAChR but may still pose an adverse risk to larval fish growth and
   neurobehavior.},
DOI = {10.1080/15287394.2022.2081641},
EarlyAccessDate = {JUN 2022},
ISSN = {1528-7394},
EISSN = {1087-2620},
Unique-ID = {WOS:000804651900001},
}

