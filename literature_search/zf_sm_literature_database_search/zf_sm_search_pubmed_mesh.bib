	note = {Place: Ireland},
	keywords = {Male, Animals, Female, Metabolomics, Gene Expression Profiling, Structure-Activity Relationship, Stereoisomerism, Gene Expression Regulation, Sex Factors, Proton Magnetic Resonance Spectroscopy, Molecular Structure, *Gene expression, Lipid Metabolism/drug effects, Transcriptome/*drug effects, *Enantioselective, Energy Metabolism/*drug effects, Liver/*drug effects/metabolism/pathology, *Zebrafish/genetics/metabolism, Fungicides, Industrial/chemistry/*toxicity, Metabolome/*drug effects, *Liver metabolomics, *Liver toxicity, *Mefentrifluconazole, Fluconazole/*analogs \& derivatives/chemistry/toxicity},
	pages = {153095},
