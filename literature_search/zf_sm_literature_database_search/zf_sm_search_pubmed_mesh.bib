	pmid = {28002765},
	note = {Place: England},
	keywords = {Humans, Animals, Tandem Mass Spectrometry, Developmental toxicity, Zebrafish, Herbicides/*toxicity, Oxidative Stress/drug effects, Zebrafish/*growth \& development/metabolism, Embryonic Development/*drug effects, 2,4-Dichlorophenoxyacetic Acid/*toxicity, 2,4-Dichlorophenoxyacetic acid, Cardiac toxicity, Embryo, Nonmammalian/*cytology/drug effects/metabolism, Embryonic toxicity},
	pages = {40--48},
