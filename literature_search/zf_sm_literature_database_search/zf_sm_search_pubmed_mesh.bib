	note = {Place: Netherlands},
	keywords = {Animals, Chlorpyrifos/*toxicity, Insecticides/*toxicity, Larva/anatomy \& histology/drug effects, Motor Activity/drug effects, Nickel/*toxicity, Toxicity Tests, Acute, Water Pollutants, Chemical/toxicity, Zebrafish/abnormalities/*embryology/*growth \& development},
	pages = {1740--1747},
}

@article{yang_cadmium_2016,
