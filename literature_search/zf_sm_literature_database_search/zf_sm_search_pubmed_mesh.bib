	doi = {10.1016/j.aquatox.2017.08.002},
	abstract = {Acetamiprid, a neonicotinoid pesticide, is reported to have adverse sublethal effects on non-target beneficial organisms. Halosulfuron-methyl (HM), one of the  most widely used herbicides in agriculture, has high ecotoxicity to aquatic  plants and animals. In this study, a GC-MS-based metabolomics approach was used  to investigate the toxicity of acetamiprid and HM. The Automated Mass Spectral  Deconvolution and Identification System (AMDIS) software program and the  retention index method were used to identify 51 metabolites in zebra fish  (Brachydanio rerio). Changes in metabolites showed that acetamiprid and HM  disturbed amino acid (e.g., leucine, valine, serine, glycine, proline, and  alanine) metabolism, the TCA cycle (malic acid and fumaric acid), and the balance  of neurotransmitters (glutamic acid, taurine, and glycine). The change in  metabolites in the liver, head, and blood indicated that metabolites in the liver  were more sensitive than those in the head and blood. Overall, on the basis of  the change in metabolites, we identified a potential risk to zebra fish exposed  to sublethal doses of acetamiprid and/or HM.},
	language = {eng},
	journal = {Aquatic toxicology (Amsterdam, Netherlands)},
	author = {Zhang, Hong and Zhao, Lijuan},
	month = oct,
	year = {2017},
	pmid = {28806601},
	note = {Place: Netherlands},
	keywords = {Animals, Metabolomics, Lethal Dose 50, Gas Chromatography-Mass Spectrometry, Dose-Response Relationship, Drug, Water Pollutants, Chemical/*toxicity, Herbicides/*toxicity, Zebrafish/*metabolism, GC–MS, Sulfonylurea Compounds/*toxicity, Amino Acids/metabolism, Neonicotinoids/*toxicity, Metabolomics/methods, Acetamiprid and halosulfuron-methyl, Metabolome/*drug effects, Zebra fish},
	pages = {85--94},
}

@article{teng_effects_2018,
	title = {Effects of the bioconcentration and parental transfer of environmentally relevant concentrations of difenoconazole on endocrine disruption in zebrafish ({Danio}  rerio).},
	volume = {233},
	copyright = {Copyright © 2017 Elsevier Ltd. All rights reserved.},
	issn = {1873-6424 0269-7491},
	doi = {10.1016/j.envpol.2017.10.063},
	abstract = {Difenoconazole, a typical triazole fungicide, inhibits lanosterol-14R-demethylase (CYP51) to prevent fungal sterol synthesis and its residues are frequently  detected in the environment due to its wide application. Previous studies have  demonstrated that difenoconazole altered the triglyceride levels, and gene  expression relevant to cholesterol biosynthesis in zebrafish. However,  endocrine-disruption in the hypothalamus-pituitary-gonadal-liver (HPGL) axis, the  effects of transferring to offspring, and the underlying mechanisms of  difenoconazole in aquatic organisms are still unknown. In this study, we defined  the effects of difenoconazole at environmental concentrations on endocrine  disturbance using zebrafish as an experimental model. The results indicated that  difenoconazole induced a significant change in the somatic index, and  pathological variations in tissues, and steroid hormone levels. RT-PCR  experiments further confirmed that difenoconazole significantly induced  expression alteration of lhr, hsd3β, hsd11β, cyp19a in the ovary and star,  cyp19a, cyp3c1 in the testis, and erα genes in livers. In addition,  difenoconazole exposure in parental zebrafish affected the hatchability and  length of its offspring. Moreover, the burdens of difenoconazole and  difenoconazole alcohol in females were higher than in males. These findings  highlighted that difenoconazole exposure at environmentally relevant  concentrations elicited estrogenic endocrine-disruption effects via altering  homeostasis of sex steroid hormones in the HPGL axis and the adverse effects can  be transferred to the offspring.},
	language = {eng},
	journal = {Environmental pollution (Barking, Essex : 1987)},
	author = {Teng, Miaomiao and Qi, Suzhen and Zhu, Wentao and Wang, Yao and Wang, Dezhen and Dong, Kai and Wang, Chengju},
	month = feb,
	year = {2018},
	pmid = {29096293},
	note = {Place: England},
	keywords = {Male, Animals, Bioaccumulation, Female, Zebrafish, Difenoconazole, Endocrine disruption, Aquatic Organisms/metabolism, Liver/metabolism, Triazoles/*toxicity, Zebrafish/metabolism/*physiology, Fungicides, Industrial/metabolism/*toxicity, Water Pollutants, Chemical/metabolism/*toxicity, Dioxolanes/*toxicity, Ovary/drug effects, Endocrine Disruptors/metabolism/*toxicity, Gonadal Steroid Hormones/metabolism, Parental transfer, Testis/drug effects},
	pages = {208--217},
}

@article{santos_da_rosa_muscarinic_2017,
	title = {Muscarinic receptors mediate the endocrine-disrupting effects of an organophosphorus insecticide in zebrafish.},
	volume = {32},
	copyright = {© 2017 Wiley Periodicals, Inc.},
	issn = {1522-7278 1520-4081},
	doi = {10.1002/tox.22424},
	abstract = {The glucocorticoid cortisol, the end product of hypothalamus-pituitary-interrenal axis in zebrafish (Danio rerio), is synthesized via steroidogenesis and promotes  important physiological regulations in response to a stressor. The failure of  this axis leads to inability to cope with environmental challenges preventing  adaptive processes in order to restore homeostasis. Pesticides and agrichemicals  are widely used, and may constitute an important class of environmental  pollutants when reach aquatic ecosystems and nontarget species. These chemical  compounds may disrupt hypothalamus-pituitary-interrenal axis by altering  synthesis, structure or function of its constituents. We present evidence that  organophosphorus exposure disrupts stress response by altering the expression of  key genes of the neural steroidogenesis, causing downregulation of star, hsp70,  and pomc genes. This appears to be mediated via muscarinic receptors, since the  muscarinic antagonist scopolamine blocked these effects.},
	language = {eng},
	number = {7},
	journal = {Environmental toxicology},
	author = {Santos da Rosa, João Gabriel and Alcântara Barcellos, Heloísa Helena de and Fagundes, Michele and Variani, Cristiane and Rossini, Mainara and Kalichak, Fabiana and Koakoski, Gessi and Acosta Oliveira, Thiago and Idalencio, Renan and Frandoloso, Rafael and Piato, Angelo L. and José Gil Barcellos, Leonardo},
	month = jul,
	year = {2017},
	pmid = {28371364},
	note = {Place: United States},
	keywords = {Animals, Danio rerio, steroidogenesis, scopolamine, Endocrine Disruptors/*toxicity, Insecticides/*toxicity, Water Pollutants, Chemical/*toxicity, RNA, Messenger/metabolism, Zebrafish/*metabolism, Locomotion/drug effects, Zebrafish Proteins/genetics/metabolism, Methyl Parathion/*toxicity, Hydrocortisone/metabolism, acetylcholine receptors, HSP72 Heat-Shock Proteins/genetics/metabolism, methyl-parathion, Muscarinic Antagonists/pharmacology, Phosphoproteins/genetics/metabolism, Pro-Opiomelanocortin/genetics/metabolism, Proto-Oncogene Proteins c-fos/metabolism, Receptors, Muscarinic/*metabolism, Scopolamine/pharmacology},
