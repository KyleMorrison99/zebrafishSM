	pmid = {30265925},
	note = {Place: England},
	keywords = {Europe, Animals, Toxicity, Melanin, Behaviour, Penthiopyrad, Fungicides, Industrial/*toxicity, Lipid Metabolism/drug effects, Water Pollutants, Chemical/toxicity, Larva/drug effects/metabolism, Zebrafish/*growth \& development/metabolism, Behavior, Animal/*drug effects, Toxicity Tests, Acute, Lipid, Pyrazoles/pharmacology/*toxicity, Succinate Dehydrogenase/*antagonists \& inhibitors, Thiophenes/pharmacology/*toxicity},
