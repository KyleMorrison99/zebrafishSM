	note = {Place: Netherlands},
	keywords = {Male, Animals, Time Factors, Xenobiotics, Drug Combinations, Drug Synergism, Half-Life, Dose-Response Relationship, Drug, Sperm Motility/drug effects, DDT/*pharmacology/toxicity, Spermatozoa/*drug effects/physiology, Zebrafish/*physiology, Chlorodiphenyl (54\% Chlorine)/*pharmacology/toxicity, Estrogens/pharmacology/toxicity, Fertilization/*drug effects, Sexual Behavior, Animal/drug effects, Sperm Count/*veterinary, Water Pollutants, Chemical/pharmacology/toxicity},
