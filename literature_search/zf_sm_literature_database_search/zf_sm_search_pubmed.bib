@article{andrade_carbendazim_2016,
	title = {Carbendazim exposure induces developmental, biochemical and behavioural disturbance in zebrafish embryos.},
	volume = {170},
	copyright = {Copyright © 2015 Elsevier B.V. All rights reserved.},
	issn = {1879-1514 0166-445X},
	doi = {10.1016/j.aquatox.2015.11.017},
	abstract = {Carbendazim is a widely used broad spectrum benzimidazole fungicide; however, its effects to non-target aquatic organisms are poorly studied. The aim of this study  was to investigate the toxic effects of carbendazim to zebrafish early life  stages at several levels of biological organization, including developmental,  biochemical and behavioural levels. The embryo assay was done following the OECD  guideline 236 and using a concentration range between 1.1 and 1.8mg/L. Lethal and  developmental endpoints such as hatching, edemas, malformations, heart beat rate,  body growth and delays were assessed in a 96h exposure. A sub-teratogenic range  (from 0.16 to 500μg/L) was then used to assess effects at biochemical and  behavioural levels. Biochemical markers included cholinesterase (ChE),  glutathione-S-transferase (GST), lactate dehydrogenase (LDH) and catalase (CAT)  and were assessed at 96h. The locomotor behaviour was assessed using an automated  video tracking system at 120h. Carbendazim (96h-LC50 of 1.75mg/L) elicited  several developmental anomalies in zebrafish embryos with EC50 values ranging  from 0.85 to 1.6mg/L. ChE, GST and LDH activities were increased at  concentrations equal or above 4μg/L. The locomotor assay showed to be extremely  sensitive, detecting effects in time that larvae spent swimming at concentrations  of 0.16μg/L and thus, being several orders of magnitude more sensitive that  developmental parameters or lethality. These are ecological relevant  concentrations and highlight the potential of behavioural endpoints as early  warning signs for environmental stress. Further studies should focus on  understanding how the behavioural disturbances measured in these types of studies  translate into fitness impairment at the adult stage.},
	language = {eng},
	journal = {Aquatic toxicology (Amsterdam, Netherlands)},
	author = {Andrade, Thayres S. and Henriques, Jorge F. and Almeida, Ana Rita and Machado, Ana Luísa and Koba, Olga and Giang, Pham Thai and Soares, Amadeu M. V. M. and Domingues, Inês},
	month = jan,
	year = {2016},
	pmid = {26653011},
	note = {Place: Netherlands},
	keywords = {Animals, Biomarkers, Tandem Mass Spectrometry, Zebrafish, Swimming, Chromatography, High Pressure Liquid, Water Pollutants, Chemical/analysis/*toxicity, Glutathione Transferase/metabolism, Catalase/metabolism, Locomotion/drug effects, Behavior, Animal/*drug effects, Zebrafish/growth \& development/*physiology, Embryo, Nonmammalian/drug effects/physiology, Larva/drug effects/physiology, Fungicides, Industrial/analysis/*toxicity, Cholinesterases/metabolism, Swimming behaviour, Benzimidazoles/analysis/*toxicity, Carbamates/analysis/*toxicity, Locomotor response, Sublethal},
	pages = {390--399},
}

@article{liu_high-throughput_2020,
	title = {High-throughput preparation of radioprotective polymers via {Hantzsch}'s reaction for in vivo {X}-ray damage determination.},
	volume = {11},
	issn = {2041-1723},
	doi = {10.1038/s41467-020-20027-0},
	abstract = {Radioprotectors for acute injuries caused by large doses of ionizing radiation are vital to national security, public health and future development of  humankind. Here, we develop a strategy to explore safe and efficient  radioprotectors by combining Hantzsch's reaction, high-throughput methods and  polymer chemistry. A water-soluble polymer with low-cytotoxicity and an excellent  anti-radiation capability has been achieved. In in vivo experiments, this polymer  is even better than amifostine, which is the only approved radioprotector for  clinical applications, in effectively protecting zebrafish embryos from fatally  large doses of ionizing radiation (80 Gy X-ray). A mechanistic study also reveals  that the radioprotective ability of this polymer originates from its ability to  efficiently prevent DNA damage due to high doses of radiation. This is an initial  attempt to explore polymer radioprotectors via a multi-component reaction. It  allows exploiting functional polymers and provides the underlying insights to  guide the design of radioprotective polymers.},
	language = {eng},
	number = {1},
	journal = {Nature communications},
	author = {Liu, Guoqiang and Zeng, Yuan and Lv, Tong and Mao, Tengfei and Wei, Yen and Jia, Shunji and Gou, Yanzi and Tao, Lei},
	month = dec,
	year = {2020},
	pmid = {33277480},
	pmcid = {PMC7718248},
	keywords = {Animals, Mice, Cell Line, Molecular Structure, Comet Assay, Models, Chemical, Zebrafish/embryology, *X-Rays, Amifostine/pharmacology, Cell Survival/drug effects/radiation effects, Chemistry Techniques, Synthetic/*methods, DNA Damage/drug effects/radiation effects, Embryo, Nonmammalian/drug effects/embryology/*radiation effects, Fibroblasts/cytology/drug effects/*radiation effects, Polymers/*chemical synthesis/chemistry/pharmacology, Radiation-Protective Agents/*chemical synthesis/chemistry/pharmacology},
