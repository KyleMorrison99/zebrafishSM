@article{liu_antimetastatic_2015,
	title = {Antimetastatic {Therapies} of the {Polysulfide} {Diallyl} {Trisulfide} against {Triple}-{Negative} {Breast} {Cancer} ({TNBC}) via {Suppressing} {MMP2}/9 by {Blocking} {NF}-κ{B} and  {ERK}/{MAPK} {Signaling} {Pathways}.},
	volume = {10},
	issn = {1932-6203},
	doi = {10.1371/journal.pone.0123781},
	abstract = {BACKGROUND: Migration and invasion are two crucial steps of tumor metastasis. Blockage of these steps may be an effective strategy to reduce the risk. The  objective of the present study was to investigate the effects of diallyl  trisulfide (DATS), a natural organosulfuric compound with most sulfur atoms found  in garlic, on migration and invasion in triple negative breast cancer (TNBC)  cells. Molecular mechanisms underlying the anticancer effects of DATS were  further investigated. METHODS AND RESULTS: MDA-MB-231 cells and HS 578t breast  cancer cells were treated with different concentrations of DATS. DATS obviously  suppressed the migration and invasion of two cell lines and changed the  morphological. Moreover, DATS inhibited the mRNA/protein/ enzymes activities of  MMP2/9 via attenuating the NF-κB pathway. DATS also inhibited ERK/MAPK rather  than p38 and JNK. CONCLUSION: DATS inhibits MMP2/9 activity and the metastasis of  TNBC cells, and emerges as a potential anti-cancer agent. The inhibitory effects  are associated with down-regulation of the transcriptional activities of NF-κB  and ERK/MAPK signaling pathways.},
	language = {eng},
	number = {4},
	journal = {PloS one},
	author = {Liu, Yuping and Zhu, Pingting and Wang, Yingyu and Wei, Zhonghong and Tao, Li and Zhu, Zhijie and Sheng, Xiaobo and Wang, Siliang and Ruan, Junshan and Liu, Zhaoguo and Cao, Yuzhu and Shan, Yunlong and Sun, Lihua and Wang, Aiyun and Chen, Wenxing and Lu, Yin},
	year = {2015},
	pmid = {25927362},
	pmcid = {PMC4415928},
	keywords = {Humans, Animals, Female, Zebrafish, Neoplasm Invasiveness, Neoplasm Metastasis, Cell Line, Tumor, Cell Movement/drug effects, Drug Screening Assays, Antitumor, Allyl Compounds/chemistry/*pharmacology, Breast Neoplasms/*drug therapy/metabolism/pathology, MAP Kinase Signaling System/*drug effects, Matrix Metalloproteinase 2/*biosynthesis, Matrix Metalloproteinase 9/*biosynthesis, Neoplasm Proteins/*metabolism, NF-kappa B/*metabolism, Sulfides/chemistry/*pharmacology},
