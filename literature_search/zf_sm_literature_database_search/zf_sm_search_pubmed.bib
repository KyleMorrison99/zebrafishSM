	number = {3},
	journal = {Ecotoxicology and environmental safety},
	author = {Na, Yi-Rang and Seok, Seung-Hyeok and Baek, Min-Won and Lee, Hui-Young and Kim, Dong-Jae and Park, Sung-Hoon and Lee, Hyun-Kyoung and Park, Jae-Hak},
	month = mar,
